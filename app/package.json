{
  "name": "vue3-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "VITE_MOCKED_API=true VITE_SERVICE_ENV=dev vite",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc --noEmit --skipLibCheck && VITE_SERVICE_ENV=dev vite build",
    "build:prod": "vue-tsc --noEmit --skipLibCheck && VITE_SERVICE_ENV=prod vite build",
    "build-storybook": "storybook build",
    "preview": "vite preview",
    "format": "biome format ./src",
    "format:write": "biome format --write ./src",
    "lint": "biome lint ./src",
    "lint:write": "biome lint --write ./src",
    "storybook": "storybook dev --port 6006",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.2",
    "element-plus": "^2.11.8",
    "path": "^0.12.7",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/vue3-vite": "^8.6.12",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.10",
    "msw": "^2.7.3",
    "playwright": "^1.49.0",
    "storybook": "^8.6.12",
    "typescript": "^5.9.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.1",
    "vue-tsc": "^3.1.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "resolutions": {
    "strip-ansi": "6.0.0"
  }
}
